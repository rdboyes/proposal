@ARTICLE{Wellman2016-yp,
  title    = "Predictors of the Onset of Cigarette Smoking: A Systematic Review
              of Longitudinal {Population-Based} Studies in Youth",
  author   = "Wellman, Robert J and Dugas, Erika N and Dutczak, Hartley and
